\n.project-link:hover:after{\r\n    transform: scaleX(1);\r\n    transform-origin: bottom left;\r\n}\r\n\r\n.disclaimer{\r\n    color: #FFEDDF !important;\r\n}\r\n\r\n.contact{\r\n    background-color: $linen !important;\r\n    background-image: url(/images/contact-1.svg);\r\n    background-repeat: no-repeat;\r\n    background-size: 65%;\r\n    background-position: right;\r\n    color: $rich-black !important;\r\n}\r\n\r\n.home,\r\n.about,\r\n.projects,\r\n.contact{\r\n  height: 90vh;\r\n  position: absolute;\r\n  width: 100%;\r\n}\r\n\r\n.home-content,\r\n.about-content,\r\n.projects-content,\r\n.contact-content{\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n}\r\n\r\nimg{\r\n    // border-radius: 50%;\r\n    border: solid $deep-taupe;\r\n    align-items: right;\r\n    size: 20%;\r\n}\r\n\r\n.nav-list{\r\n  height: 10vh;\r\n  display: flex;\r\n  // width: 45%;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  margin-left: auto;\r\n  list-style: none;\r\n  background-color: $rich-black;\r\n  //margin: 0px !important;\r\n  //padding: 0px !important;\r\n  //box-sizing: border-box !important;\r\n}\r\n\r\n.nav-links{\r\n  text-decoration: none;\r\n  font-size: 18px;\r\n  transition: all 0.5s;\r\n}\r\n\r\n.nav-links:hover{\r\n  background-color: #C5D86D !important;\r\n  color: #0D1321 !important;\r\n  transition: all 0.5s;\r\n  padding-top: 100% !important;\r\n  padding-bottom: 50% !important;\r\n  padding-left: 30% !important;\r\n  padding-right: 30% !important;\r\n}\r\n\r\na{\r\n    color: $june-bud !important;\r\n}\r\n\r\n.fa-brands{\r\n    color: $june-bud !important;\r\n}\r\n\r\n.name-and-location{\r\n    color: $june-bud !important;\r\n    // padding-left: 5% !important;\r\n    // padding-bottom: 30% !important;\r\n}\r\n\r\n.my-blurb{\r\n    color: $linen !important;\r\n    // padding-left: 5% !important;\r\n    \r\n}\r\n\r\n.my-bio{\r\n    // padding-bottom: 30% !important;\r\n    font-weight