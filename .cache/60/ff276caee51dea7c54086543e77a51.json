n    color: #FFEDDF !important;\r\n}\r\n\r\n.contact{\r\n    background-color: $linen !important;\r\n    background-image: url(/images/contact-1.svg);\r\n    background-repeat: no-repeat;\r\n    background-size: 65%;\r\n    background-position: right;\r\n    color: $rich-black !important;\r\n}\r\n\r\n.home,\r\n.about,\r\n.projects,\r\n.contact{\r\n  height: 90vh;\r\n  position: absolute;\r\n  width: 100%;\r\n}\r\n\r\n\r\n.home-content,\r\n.about-content,\r\n.projects-content,\r\n.contact-content{\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: center;\r\n}\r\n\r\n.personal-about,\r\n.tech-about{\r\n    background-color: rgba($linen, .6);\r\n    color: $rich-black;\r\n}\r\n\r\n.lang-icon{\r\n    color: $rich-black !important;\r\n}\r\n\r\n.contact-banner{\r\n    font-weight: bold !important;\r\n}\r\n\r\n.contact-blurb{\r\n    color: #86615C !important;\r\n}\r\n\r\n.nav-list{\r\n  height: 10vh;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  margin-left: auto;\r\n  list-style: none;\r\n  background-color: $rich-black;\r\n}\r\n\r\n.nav-links{\r\n  text-decoration: none;\r\n  font-size: 18px;\r\n  transition: all 0.5s;\r\n}\r\n\r\n.nav-links:hover{\r\n  background-color: #C5D86D !important;\r\n  color: #0D1321 !important;\r\n  transition: all 0.5s;\r\n  padding-top: 100% !important;\r\n  padding-bottom: 50% !important;\r\n  padding-left: 30% !important;\r\n  padding-right: 30% !important;\r\n}\r\n\r\na{\r\n    color: $june-bud !important;\r\n}\r\n\r\n.name-and-location{\r\n    color: $june-bud !important;\r\n}\r\n\r\n.my-blurb{\r\n    color: $linen !important;\r\n    \r\n}\r\n\r\n.sg-link{\r\n    color: #0D1321 !important;\r\n}\r\n\r\nfooter{\r\n    bottom:0;\r\n    height: 75px;   /* Height of the footer */\r\n }\r\n\r\n input{\r\n   background: transparent !important;\r\n   border: none;\r\n   border-bottom: #0D1321 solid thin;\r\n   padding: 10px !important;\r\n   margin: 10px !important;\r\n   font-weight: bold !important;\r\n }\r\n\r\n label{\r\n   font-weight: bold;\r\n }\r\n\r\n .form-group{\r\n   background-color: #C5D86D !important;\r\n }\r\n\r\n"},"lineCount":null}},"error":null,"hash":"160fa0645590c1f898eed3c14974e08b","cacheData":{"env":{}}}